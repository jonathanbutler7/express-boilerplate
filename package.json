{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/helpers.spec.js tests/scores.spec.js tests/app.spec.js",
    "coverage": "nyc mocha tests/helpers.spec.js tests/scores.spec.js tests/app.spec.js",
    "dev": "nodemon src/server.js",
    "seed-db": "psql -U postgres -f ./seeds/sql/seed.create_db.sql",
    "seed-tables": "psql -U postgres -d benchmark -f ./seeds/sql/seed.tables.sql",
    "env": "touch .env",
    "pg": "echo 'DATABASE_URL=postgresql://postgres@localhost/benchmark' >> .env",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "knex": "^0.21.17",
    "morgan": "^1.10.0",
    "nyc": "^15.1.0",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}